@{
    ViewData["Title"] = "UserProfile";
}

@section Styles
    {
    <style type="text/css">
        body {
            background-color: lightgray;
        }
        img{
            mix-blend-mode:multiply;
        }
    </style>
}

<div class="container">
    <div class="row">
        <div class="col-6 offset-3">
            <h1 class="text-center">My Profile</h1>
            <div class="card">                
                <div class="card-header text-center">
                    @if (Model.Gender == "male")
                    {
                        <img src="data:image/jpeg;base64," />
                    }
                    else
                    {
                        <img src="data:image/jpeg;base64," />
                    }
                </div>   
                
                <div class="card-body">
                    <h5 class="text-info text-center m1">User Information</h5>
                    <table class="text-center table table-bordered">
                        <tr><td>Username:</td><td class="text-info">@Model.Username.ToUpper()</td></tr>
                        <tr><td>Email:</td><td class="text-info">@Model.Email</td></tr>
                        <tr><td>Phone Number:</td><td class="text-info">@Model.Mobile</td></tr>
                        <tr><td>Gender:</td><td class="text-info">@Model.Gender.ToUpper()</td></tr>
                        <tr><td>City:</td><td class="text-info">@Model.City</td></tr>
                        <tr><td>Country:</td><td class="text-info">@Model.Country.ToUpper()</td></tr>
                    </table>
                </div>

            </div>
        </div>
    </div>
</div>